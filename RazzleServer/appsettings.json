{
  "RazzleServerConfig": {
    "CacheFolder": "../Data/DataCache",
    "WzFilePath": "../Data/WZ",
    "PrintPackets": true,
    "Worlds": [{
      "Id": 0,
      "Name": "Tespia",
      "Channels": 3,
      "Flag": "None",
      "EventMessage": "",
      "TickerMessage": "",
      "EnableCharacterCreation": true,
      "ExperienceRate": 10,
      "QuestExperienceRate": 10,
      "PartyQuestExperienceRate": 10,
      "MesoRate": 10,
      "DropRate": 10
    }]
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Error",
        "Microsoft.Extensions": "Information",
        "System": "Debug"
      }
    },
    "WriteTo": [{
      "Name": "Logger",
      "Args": {
        "configureLogger": {
          "WriteTo": [{
              "Name": "File",
              "Args": {
                "path": "Logs/RazzleServer.log",
                "rollingInterval": "Day"
              }
            },
            {
              "Name": "Console",
              "Args": {
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
              }
            }
          ]
        }
      }
    }]
  }
}